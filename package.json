{
  "name": "extract-front-matter-properties",
  "version": "0.0.0",
  "keywords": [
    "extract",
    "frontmatter",
	 "front",
	 "matter",
    "properties",
    "markdown",
    "filesystem"
  ],
  "main": "dist/index.js",
  "browser": "dist-browser/index.js",
  "author": "Kennedy Rose <krose@escaladesports.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/escaladesports/extract-front-matter-properties.git"
  },
  "license": "MIT",
  "scripts": {
    "analyze": "esca-scripts analyze --input './src/inject.js' --browser",
    "build": "esca-scripts build --input './src/index.js' --output './dist/index.js' --babel --component",
    "dev": "esca-scripts dev ---input './dev/dev.js, ./dev/index.html' --browser",
    "prepublishOnly": "npm run build",
    "reset": "esca-scripts reset",
    "test": "esca-scripts test",
    "travis": "opn https://travis-ci.org/escaladesports/extract-front-matter-properties/branches"
  },
  "devDependencies": {
    "esca-scripts": "^0.13.1",
    "npm-run-all": "^4.1.2"
  },
  "dependencies": {
    "fs-extra": "^5.0.0"
  }
}
